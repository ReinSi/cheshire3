    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/replaces"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
