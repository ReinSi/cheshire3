  <!-- Define our primary classes -->
  <owl:Class rdf:about="http://www.openarchives.org/ore/terms/Aggregation">
    <rdfs:label>Aggregation</rdfs:label>
    <rdfs:comment>A set of related resources (Aggregated Resources), grouped together such that the set can be treated as a single resource. This is the entity described within the ORE interoperability framework by a Resource Map.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/dcmitype/Collection" />
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/ore/terms/"/>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment>Allows for edge case of an empty aggregation, eg aggregation of super models that editor has dated</rdfs:comment>
        <owl:onProperty rdf:resource="http://www.openarchives.org/ore/terms/aggregates"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment>RECOMMENDED minCardinality 1, as required for Atom serialization</rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/title"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment>SHOULD be used when Aggregation is an Aggregated Resource</rdfs:comment>
        <owl:onProperty rdf:resource="http://www.openarchives.org/ore/terms/isDescribedBy"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment>RECOMMENDED minCardinality 1, as otherwise in Atom serialization will default to author of the *feed*, which is likely incorrect.</rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/creator"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://www.openarchives.org/ore/terms/similarTo"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/description"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/contributor"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/conformsTo"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <rdfs:comment></rdfs:comment>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/created"/>
        <owl:minCardinality>0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
